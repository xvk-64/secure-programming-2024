{
  "name": "@sp24/server",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p ./src/tsconfig.json",
    "dev": "npm-run-all build --parallel dev:tsc dev:run",
    "dev:tsc": "tsc -p ./src/tsconfig.json --watch",
    "dev:run": "node --watch ./dist/server.js",
    "clean": "node ./_clean.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/ws": "^8.5.12",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.0"
  },
  "dependencies": {
    "@sp24/common": "^0.0.0",
    "express": "^4.19.2",
    "ws": "^8.18.0"
  }
}
